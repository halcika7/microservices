{
  "name": "orders",
  "version": "1.0.0",
  "main": "dist/index.js",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "clean": "rimraf dist && mkdir dist",
    "build": "npm-run-all clean build:types build:js copy-static-assets",
    "build:types": "tsc -p tsconfig.prod.json --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts\"",
    "copy-static-assets": "ts-node copy-static.ts",
    "lint": "eslint --resolve-plugins-relative-to . --ext .ts --fix",
    "format": "prettier --write . --ignore-path",
    "test": "jest --runInBand --no-cache",
    "test:coverage": "yarn test --coverage --detectOpenHandles"
  },
  "devDependencies": {
    "@shopify/eslint-plugin": "^41.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.4",
    "@types/shelljs": "^0.8.10",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "better-docs": "^2.3.2",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^27.4.5",
    "jsdoc": "^3.6.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "shelljs": "^0.8.4",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/node": "^7.16.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@babel/runtime": "^7.16.5",
    "@halcika-micro/common": "0.0.15",
    "@halcika-micro/errors": "latest",
    "@halcika-micro/logger": "latest",
    "babel-plugin-root-import": "^6.6.0",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "mongoose": "^6.1.4",
    "mongoose-update-if-current": "^1.4.0",
    "nats": "^2.4.0",
    "node-nats-streaming": "^0.3.2"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverage": false,
    "testMatch": [
      "**/*.test.ts"
    ],
    "setupFiles": [
      "<rootDir>/jest/env.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest/setup.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!src/types/**",
      "!src/models/**",
      "!src/routes/index.ts",
      "!src/index.ts"
    ]
  }
}
