{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": "\\.(js|ts)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown",
    "../../node_modules/better-docs/category",
    "../../node_modules/better-docs/typescript"
  ],
  "templates": {
    "better-docs": {
      "name": "Node API"
    }
  },
  "opts": {
    "destination": "docs",
    "recurse": true,
    "readme": "README.md",
    "template": "../../node_modules/better-docs"
  }
}
